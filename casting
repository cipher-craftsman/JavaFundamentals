import java.util.Arrays;

public class Main {

    public static void  main(String[] args) {
        //implicit casting
        // byte > short > int > long > float > double
        byte x = 1;
        int y = x + 1;
        System.out.println(y);

        //explicit casting
        double a = 1.1;
        double b = x + (int)2;
        System.out.println(b);

        //parse Strings
        String g = "1";
        int h = Integer.parseInt(g) + 2;
        System.out.println(h);


    }
}
